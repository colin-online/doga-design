    "react-scripts": "5.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
